{
    "extends": "./tsconfig.base.json",
    "compilerOptions": {
        "paths": {
            "#~src/*": [
                "./src/*"
            ],
            "#~fourtune/*": [
                "./auto/fourtune/src/*"
            ],
            "#~auto/*": [
                "./auto/user/src/*"
            ],
            "#~synthetic/*": [
                "./auto/synthetic/src/*"
            ],
            "#~assets/*": [
                "./assets/*"
            ]
        }
    },
    "include": [
        "../../../src/**/*"
    ]
}
